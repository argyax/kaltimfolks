{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/search/page.tsx",
    "src/app/api/search/route.js",
    "src/app/api/categories/route.js",
    "src/app/api/posts/route.js",
    "src/app/components/Post/post.jsx",
    "src/app/admin/dashboard/fshy/func.jsx"
, "src/app/components/CardBlog/CardBlog.jsx"  ],
  "exclude": ["node_modules"]
}
